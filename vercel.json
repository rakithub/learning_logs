{
    "builds": [{
        "src": "/learning_log/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "output": "/learning_logs/output"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/learning_log/wsgi.py"
        }
    ]
}